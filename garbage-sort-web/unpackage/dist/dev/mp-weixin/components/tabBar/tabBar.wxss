.tabBar-root {
  background-color: #fff;
  width: 100%;
  align-self: flex-end;
  display: flex;
  justify-content: space-around;
  height: 80px;
  border-top: 1px solid #efefef;
}
.tabBar-item {
  position: relative;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.tabBar-item image {
  width: 30px;
  height: 30px;
}
.tabBar-item text {
  display: block;
  font-size: 0.8rem;
}
.mid-btn {
  border-radius: 50%;
  width: 50px !important;
  height: 50px !important;
  position: absolute;
  bottom: 40%;
  background-color: #fff;
  padding: 0.7rem;
  border-top: 1px solid #efefef;
}
.aside-image {
  position: relative;
  bottom: 10px;
}

